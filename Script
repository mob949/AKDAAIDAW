GravityCoil.Font = Enum.Font.SourceSansBold
GravityCoil.Text = "Gravity Coil"
GravityCoil.TextColor3 = Color3.fromRGB(255, 0, 255)
GravityCoil.TextSize = 25.000
GravityCoil.MouseButton1Down:connect(function()
	game:service'ReplicatedStorage'.GravityCoilEvent:FireServer("GET");
end)

